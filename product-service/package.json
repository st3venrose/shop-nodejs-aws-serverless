{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "sls:deploy:prod": "serverless deploy -s prod",
    "sls:deploy:dev": "serverless deploy -s dev",
    "sls:package": "serverless package",
